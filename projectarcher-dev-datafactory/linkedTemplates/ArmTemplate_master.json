{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "projectarcher-dev-datafactory"
		},
		"AzureFunctionActivity_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'AzureFunctionActivity'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"OutputSqlDataset_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OutputSqlDataset'"
		},
		"AzureFunctionActivity_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://azurefunctiontransofrmdata.azurewebsites.net"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/757515e2-2af1-48dc-ae9a-d2893609f8a4/resourceGroups/projectarcher-dev/providers/Microsoft.Storage/storageAccounts/archerblobstorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "projectarcher-dev-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureFunctionActivity_functionKey": {
						"value": "[parameters('AzureFunctionActivity_functionKey')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"OutputSqlDataset_connectionString": {
						"value": "[parameters('OutputSqlDataset_connectionString')]"
					},
					"AzureFunctionActivity_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('AzureFunctionActivity_properties_typeProperties_functionAppUrl')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}